/*
	Test melody ("E-eeh uhnem!", russian folk song)
*/
melody test {
	
	// main tune
	play tune main {
		play c6; //72;
		go;
		play a5; //69;
		go;
		play d6; //74;
		go 2;
		play a5 2; //69
		go 3;
	}

	play tune main;
	
	// the ending
	play c6; //72
	go;
	play f6; //77
	go;
	play e6; //76
	go;
	play d6; //74
	go;

	play tune main;
	
	// the end of melody
}